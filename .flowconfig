[include]

[ignore]
# ignore ignored nuclide deps
.*/node_modules/fbjs/lib/.*
./node_modules/nuclide/node_modules/rxjs/.*
./node_modules/nuclide/node_modules/react/react.js
./node_modules/nuclide/node_modules/babel-types/.*
./node_modules/nuclide/node_modules/sabor/.*
./node_modules/nuclide/nuclide-node-transpiler/spec/fixtures/.*
./node_modules/nuclide/node_modules/json5/.*json
# silence temporary issue
./node_modules/nuclide/pkg/nuclide-hg-repository-base/.*

[libs]
./node_modules/nuclide/pkg/nuclide-external-interfaces/1.0/

[options]
suppress_comment=.*\\$FlowIgnore.*
esproposal.class_static_fields=enable
esproposal.decorators=ignore
unsafe.enable_getters_and_setters=true
module.use_strict=true

[version]
0.24.1
